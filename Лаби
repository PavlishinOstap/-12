a = input("Ведіть перше число: ")
b = input("Ведіть друге число: ")
c = input("Що робимо?(= - * /): ")
if c == "+":
 print(int(a) + int(b))
if c == "-":
 print(int(a) - int(b))
if c == "*":
 print(int(a) * int(b))
if c == "/":
 print(int(a) / int(b))
